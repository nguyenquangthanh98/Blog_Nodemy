<script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
    crossorigin="anonymous"></script>
<script src="https://cdn.quilljs.com/1.3.6/quill.js"></script>

<script>

    var quill = new Quill('#editor');

    // var ops = [{"attributes":{"bold":true},"insert":"asdad"},{"insert":"\nsr"},{"attributes":{"italic":true},"insert":"ffsfs"},{"insert":"\n"}]

    // quill.setContents(ops, 'api');

    $.ajax({
        url: "/api/get-all-post",
        type: "get"
    }).then(function (data) {
        var ops = data.data;
        // console.log(ops);

        var content = []

        ops.forEach(element => {
            var value = element.content[0]
            content = content.concat(JSON.parse(value))
            // console.log(value);
            console.log(JSON.parse(value));
        });
        quill.setContents(content, 'api');

        // quill.setContents({ "ops": [{ "insert": data }] }, 'api');

    })

</script>

